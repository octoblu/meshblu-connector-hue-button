{
  "schemas": {
    "version": "1.0.0",
    "form": {},
    "configure": {
      "my-default-config": {
        "type": "object",
        "properties": {
          "options": {
            "type": "object",
            "properties": {
              "ipAddress": {
                "type": "string",
                "required": true
              },
              "apiUsername": {
                "type": "string",
                "required": true,
                "default": "newdeveloper"
              },
              "sensorName": {
                "type": "string",
                "required": true,
                "default": "Hue Tap 1"
              },
              "sensorPollInterval": {
                "type": "integer",
                "required": true,
                "default": 5000
              }
            }
          }
        }
      }
    },
    "message": {}
  }
}
